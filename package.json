  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^11.1.0",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "multiple-cucumber-html-reporter": "^1.19.0"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "json": {
        "enabled": true,
        "output": "cypress/cucumber-json/cucumber-report.json"
      }
    },
    "stepDefinitions": [
      "cypress/e2e/*.{js,ts}",
      "cypress/e2e/**/*.{js,ts}",
      "[filepath]/**/*.{js,ts}",
      "[filepath]/**/**/**/*.{js,ts}",
      "[filepath].{js,ts}",
      "cypress/e2e/**/**/*.{js,ts}",
      "cypress/e2e/**/*.{js,ts}",
      "[filepath]/**/*.{js,ts}",
      "[filepath].{js,ts}",
      "[filepart]/step_definitions/**/*.{js,ts}",
      "[filepart]/step_definitions/*.{js,ts}",
      "cypress/support/step_definitions/**/*.{js,ts}",
      "cypress/support/step_definitions/*.{js,ts}",
      "cypress/e2e/StepDefinitions/**/*.js"
    ],
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
   
  }
}
